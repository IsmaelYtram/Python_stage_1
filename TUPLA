#                                                       TUPLA

nome = (2,3,4,5,1,2)
print(type(nome))

for i in range(len(nome)):
    print(nome[i])
print("\n")
print(len(nome))

print("\n",nome[2])

print(max(nome))
print(sum(nome))
print(8 or 2 in nome)

nome1 = 1,2,3,4,5

print(sum(nome + nome1))
print(max(nome+nome1))
print(min(nome+nome1))

tupla = 1,2,3,4,5,6
print(tupla[2:])
